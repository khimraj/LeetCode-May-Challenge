/* Approach - Use Hash map */

// Time Complexity - O(mn), where m = length 0f J and n = length of S
// Space Complexity - O(m)

class Solution {
public:
    int numJewelsInStones(string J, string S) {
        unordered_map<char,int>count;
        for (char c : J) {
            count[c] = 1;
        }
        int stones = 0;
        for (char c : S) {
            if (count[c]) {
                stones++;
            }
        }
        return stones;
    }
};