class Solution {
public:
    int numJewelsInStones(string J, string S) {
        unordered_map<char,int>count;
        for (char c : J) {
            count[c] = 1;
        }
        int stones = 0;
        for (char c : S) {
            if (count[c]) {
                stones++;
            }
        }
        return stones;
    }
};